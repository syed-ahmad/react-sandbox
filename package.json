{
  "name": "react-sandbox",
  "version": "1.0.0",
  "description": "A place to experiment with React et al",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all format lint prod:build",
    "build-monitor": "nodemon --config nodemon.json",
    "dev": "webpack-dev-server --mode development",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "prod:build": "webpack --build --mode production",
    "start": "serve -s dist",
    "test": "echo 'No tests yet...' && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syed-ahmad/react-sandbox.git"
  },
  "keywords": [
    "react",
    "playground",
    "experiment",
    "sandbox",
    "react-sandbox"
  ],
  "author": "Syed Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syed-ahmad/react-sandbox/issues"
  },
  "homepage": "https://github.com/syed-ahmad/react-sandbox#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "serve": "^11.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
